{
    "dependencies": {
        "git": {
            "version": "latest"
        },
        "node": {
            "version": "latest"
        },
        "pnpm": {
            "version": "latest"
        }
    },
    "installation": [
        {
            "name": "Cloning repository",
            "commands": [
                "git clone -b stable https://github.com/stackblitz-labs/bolt.diy bolt"
            ]
        },
        {
            "name": "Installing dependencies",
            "commands": [
                "cd bolt",
                "pnpm install"
            ]
        }
    ],
    "start": [
        {
            "name": "Starting bolt",
            "catch": "5123",
            "commands": [
                "cd bolt",
                "pnpm run dev"
            ]
        }
    ]
}
