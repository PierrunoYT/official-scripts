{
    "dependencies": {
        "git": {
            "version": "latest"
        }
    },
    "installation": [
        {
            "name": "Installing ace-step",
            "env": {
                "name": "env",
                "type": "uv",
                "version": "3.10"
            },
            "commands": [
                "git clone https://github.com/ace-step/ACE-Step.git ace-step",
                "cd ace-step",
                "uv pip install torch==2.8.0 torchvision==0.23.0 torchaudio==2.8.0 --index-url https://download.pytorch.org/whl/cu126",
                "uv pip install -e ."
            ]
        }
    ],
    "start": [
        {
            "name": "Starting ace-step",
            "catch": "7865",
            "env": {
                "name": "env",
                "type": "uv",
                "version": "3.10"
            },
            "commands": [
                "cd ace-step",
                "acestep --port 7865"
            ]
        }
    ]
}