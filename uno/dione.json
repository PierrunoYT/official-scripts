{
    "dependencies": {
        "git": {
            "version": "latest"
        }
    },
    "installation": [
        {
            "name": "Cloning repository",
            "commands": [
                "git clone https://github.com/bytedance/UNO.git uno"
            ]
        },
        {
            "name": "Installing dependencies",
            "env": {
                "name": "env",
                "type": "uv",
                "version": "3.10"
            },
            "commands": [
                "cd uno",
                "pip install -r requirements.txt"
            ]
        }
    ],
    "start": [
        {
            "name": "Running UNO",
            "catch": "7860",
            "env": {
                "name": "env",
                "type": "uv",
                "version": "3.10"
            },
              "commands": [
                "cd uno",
                "uv run app.py"
              ]
        }
    ]
}
