{
    "dependencies": {
        "git": {
            "version": "latest"
        },
        "ffmpeg": {
            "version": "latest"
        }
    },
    "installation": [
        {
            "name": "Installing FaceFusion",
            "env": {
                "name": "env",
                "type": "uv",
                "python": "3.12"
            },
            "commands": [
                "git clone https://github.com/facefusion/facefusion facefusion",
                "cd facefusion",
                "uv run install.py --onnxruntime default"
            ]
        }
    ],
    "start": [
        {
            "name": "Starting FaceFusion",
            "catch": "7865",
            "env": {
                "name": "env",
                "type": "uv",
                "python": "3.12"
            },
            "commands": [
                "cd facefusion",
                "uv run facefusion.py run --open-browser"
            ]
        }
    ]
}
